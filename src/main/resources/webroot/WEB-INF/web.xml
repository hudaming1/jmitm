<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<!-- pipe list -->
	<servlet>
		<servlet-name>pipeListServelet</servlet-name>
		<servlet-class>org.hum.wiretiger.console.servlet.PipeListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>pipeListServelet</servlet-name>
		<url-pattern>/pipe/list</url-pattern>
	</servlet-mapping>
	<!-- pipe detail -->
	<servlet>
		<servlet-name>pipeDetailServlet</servlet-name>
		<servlet-class>org.hum.wiretiger.console.servlet.PipeDetailServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>pipeDetailServlet</servlet-name>
		<url-pattern>/pipe/get</url-pattern>
	</servlet-mapping>
	<!-- connection list -->
	<servlet>
		<servlet-name>ConnectionListServelet</servlet-name>
		<servlet-class>org.hum.wiretiger.console.servlet.ConnectionListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ConnectionListServelet</servlet-name>
		<url-pattern>/conn/list</url-pattern>
	</servlet-mapping>
	<!-- connection detail -->
	<servlet>
		<servlet-name>ConnectionDetailServlet</servlet-name>
		<servlet-class>org.hum.wiretiger.console.servlet.ConnectionDetailServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ConnectionDetailServlet</servlet-name>
		<url-pattern>/conn/get</url-pattern>
	</servlet-mapping>
	<!-- download_cert -->
	<servlet>
		<servlet-name>certDownloadServlet</servlet-name>
		<servlet-class>org.hum.wiretiger.console.servlet.CertDownloadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>certDownloadServlet</servlet-name>
		<url-pattern>/cert/download</url-pattern>
	</servlet-mapping>
</web-app>